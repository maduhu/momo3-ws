{
  "gitbook": "^2.3.0",
  "title": "MoMo workshop",
  "description": "MoMo WebGIS workshop materials",
  "language": "en",
  "variables": {
    "workshopName": "Introduction to core technologies behind the MoMo geoportal",
    "workshopUrl": "http://terrestris.github.io/momo3-ws/",
    "workshopDownloadUrl": "http://terrestris.github.io/momo3-ws/momo3-ws.zip",
    "workshopPdfUrl": "http://terrestris.github.io/momo3-ws/momo3-ws_en.pdf",
    "workshopEpubUrl": "http://terrestris.github.io/momo3-ws/momo3-ws_en.epub",
    "geoServerBaseUrl": "http://localhost/geoserver",
    "extWorkshopMaterialsUrl": "http://terrestris.github.io/momo3-ws/en/extjs/materials.tar.gz",
    "extWorkshopMaterialsFileName": "materials.tar.gz",
    "extWorkshopFolderName": "ext-workshop",
    "extWorkshopUrl": "http://0.0.0.0:8000",
    "authors": [{
      "name": "Marc Jansen",
      "mail": "jansen@terrestris.de"
    }, {
      "name": "Daniel Koch",
      "mail": "koch@terrestris.de"
    }]
  },
  "links": {
    "sidebar": {
      "Select language…": "javascript:(function(l,h){l[h]=/localhost/.test(l[h])?'/':'/momo3-ws/'}(location,'href'))"
    }
  },
  "plugins": [
    "advanced-emoji",
    "edit-link",
    "exercises-tmp-fixed-6",
    "expandable-chapters",
    "image-captions",
    "include-codeblock",
    "richquotes",
    "sectionx"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/terrestris/momo3-ws/edit/master/src",
      "label": "Edit…"
    },
    "image-captions": {
      "caption": "_CAPTION_"
    },
    "richquotes": {
      "task": {
        "alert": "info",
        "picto": "fa-file-code-o"
      },
      "exercise": {
        "alert": "info",
        "picto": "fa-file-code-o"
      }
    }
  }
}
